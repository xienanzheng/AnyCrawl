{
    "name": "@anycrawl/libs",
    "version": "1.0.0-beta.2",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "clean": "rm -rf dist",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --detectOpenHandles",
        "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --detectOpenHandles --forceExit",
        "typecheck": "tsc --noEmit"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./html-to-markdown": {
            "import": "./dist/html-to-markdown.js",
            "default": "./dist/html-to-markdown.js"
        },
        "./log": {
            "import": "./dist/log.js",
            "default": "./dist/log.js"
        },
        "./types/template-config": {
            "import": "./dist/types/template-config.js",
            "default": "./dist/types/template-config.js"
        },
        "./types/BaseSchema": {
            "import": "./dist/types/BaseSchema.js",
            "default": "./dist/types/BaseSchema.js"
        },
        "./types/ScrapeSchema": {
            "import": "./dist/types/ScrapeSchema.js",
            "default": "./dist/types/ScrapeSchema.js"
        },
        "./types/CrawlSchema": {
            "import": "./dist/types/CrawlSchema.js",
            "default": "./dist/types/CrawlSchema.js"
        },
        "./types/SearchSchema": {
            "import": "./dist/types/SearchSchema.js",
            "default": "./dist/types/SearchSchema.js"
        },
        "./types/ApiTypes": {
            "import": "./dist/types/ApiTypes.js",
            "default": "./dist/types/ApiTypes.js"
        },
        "./types/WebhookEvents": {
            "types": "./dist/types/WebhookEvents.d.ts",
            "import": "./dist/types/WebhookEvents.js",
            "default": "./dist/types/WebhookEvents.js"
        },
        "./constants": {
            "import": "./dist/constants.js",
            "default": "./dist/constants.js"
        }
    },
    "devDependencies": {
        "@anycrawl/typescript-config": "workspace:*",
        "ts-jest": "^29.3.2",
        "@types/turndown": "^5.0.5",
        "@types/jest": "^29.5.14",
        "@types/express": "^5.0.1",
        "jest": "^29.7.0",
        "@jest/globals": "^29.7.0",
        "typescript": "5.8.2"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.826.0",
        "@aws-sdk/s3-request-presigner": "^3.826.0",
        "crawlee": "^3.13.0",
        "turndown": "^7.2.0",
        "zod": "^3.23.8"
    },
    "peerDependencies": {
        "@anycrawl/scrape": "workspace:*",
        "@anycrawl/search": "workspace:*"
    }
}
